{
    "name": "pobo/php-sdk",
    "description": "Official PHP SDK for Pobo API V2 - product content management and webhooks",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "pobo",
        "api",
        "sdk",
        "ecommerce",
        "product-content"
    ],
    "authors": [
        {
            "name": "Pobo",
            "email": "support@pobo.cz"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Pobo\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pobo\\Sdk\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
